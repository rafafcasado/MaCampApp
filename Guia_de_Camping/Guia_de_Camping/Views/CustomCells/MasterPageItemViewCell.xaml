<?xml version="1.0" encoding="utf-8" ?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Aspbrasil.Views.Menu.MasterPageItemViewCell">
    <ViewCell.View>
            <StackLayout Padding="15" Spacing="15" Orientation="Horizontal">
                <Image x:Name="imIcon" HeightRequest="15" />
                <Label x:Name="lbTexto" FontSize="14" FontAttributes="Bold" VerticalOptions="Center" />
        </StackLayout>
    </ViewCell.View>
</ViewCell>